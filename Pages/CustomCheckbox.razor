@code {
    [Parameter]
    public bool Value { get; set; }

    [Parameter]
    public EventCallback<bool> ValueChanged { get; set; }

    public void SetValue(bool value)
    {
        Value = value;
        ValueChanged.InvokeAsync(value);
    }
}

<InputCheckbox @bind-Value="Value" class="form-check-input" disabled />
